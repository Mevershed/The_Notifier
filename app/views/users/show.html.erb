
  <span id="dashboard-header"> My Dashboard </span>

  <div id="user-links">
    <%= link_to "My Notifications", notifications_url %>
    <%= link_to "My Calendar", calendars_url %>
  </div>

  <div class="user-info">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Name: </strong>
      <%= @user.name %>
    </p>

    <p>
      <strong>Email: </strong>
      <%= @user.email %>
    </p>

    <p>
      <strong>Address: </strong>
      <%= @user.address %>
    </p>

    <p>
      <strong>Phone Number: </strong>
      <%= @user.phone %>
    </p>

    <p>
      <strong> Emergency Contact: </strong>
      <%= @user.emergency_contact %>
    </p>

    <p>
      <strong> Shift Preference </strong>
      <%= @user.shift_preference %>
    </p>
  </div>

    <%= link_to 'Edit', edit_user_path(@user) %> |
    <%= link_to 'Back', user_path(@user) %>
